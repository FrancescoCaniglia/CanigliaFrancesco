	<bean class="my.hospitalfacade.data.HospitalPersonData">
	<property name="cf" type="String"/>
	<property name="name" type="String"/>
	<property name="surname" type="String"/>
	<property name="birthPlace" type="String"/>
	<property name="birthDate" type="java.util.Date"/>
	<property name="gender" type="String"/>
	</bean>
	
	<bean class="my.hospitalfacade.data.DoctorData" extends="my.hospitalfacade.data.HospitalPersonData">
	<property name="code" type="String"/>
	<property name="recordNumber" type="String"/>
	</bean>
	
		<bean class="my.hospitalfacade.data.PhysicalExaminationData">
	<property name="code" type="String"/>
	<property name="doctor" type="my.hospitalfacade.data.DoctorData"/>
	<property name="result" type="String"/>
	<property name="date" type="java.util.Date"/>	
	</bean>
	
	<bean class="my.hospitalfacade.data.PatientData" extends="my.hospitalfacade.data.HospitalPersonData">
	<property name="code" type="String"/>
	<property name="enterDate" type="java.util.Date"/>
	<property name="escDate" type="java.util.Date"/>
	<property name="days" type="Integer"/>
	<property name="recoveryStatus" type="String"/>
	<property name="doctor" type="my.hospitalfacade.data.DoctorData"/>
	<property name="examsList" type="java.util.List&lt;my.hospitalfacade.data.PhysicalExaminationData>"/>
	</bean>

</beans>
