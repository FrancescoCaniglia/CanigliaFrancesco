<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="beans.xsd">

<!-- beans che forniscono i Data per la manipolazione dei dati -->	
	<bean class="my.universityuiip.data.StudentData">
		<property name="serialNumber" type="java.lang.String" />
		<property name="fullName" type="java.lang.String" />
		<property name="birthDate" type="java.util.Date" />
	</bean>

	<bean class="my.universityuiip.data.ProfessorData">
		<property name="serialNumber" type="java.lang.String" />
		<property name="fullName" type="java.lang.String" />
		<property name="birthDate" type="java.util.Date" />
	</bean>

	<bean class="my.universityuiip.data.SubjectData">
		<property name="uid" type="java.lang.String" />
		<property name="name" type="java.lang.String" />
		<property name="program" type="java.lang.String" />
	</bean>

	<bean class="my.universityuiip.data.ExamData">
		<property name="uid" type="java.lang.String" />
		<property name="subject" type="my.universityuiip.data.SubjectData" />
		<property name="result" type="Enum" />
		<property name="grade" type="java.lang.Integer" />
	</bean>


</beans>
