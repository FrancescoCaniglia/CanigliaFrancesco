	<bean class="my.cyclingraceFacade.data.CyclistData">
	<property name="code" type="String"/>
	<property name="fullName" type="String"/>
	<property name="age" type="Integer"/>
	<property name="nationality" type="String"/>
	<property name="wight" type="String"/>
	<property name="team" type="String" />
	</bean>
	
	
	<bean class="my.cyclingraceFacade.data.StageRankingData">
	<property name="code" type="String"/>
	<property name="ranking" type="Integer"/>
	<property name="timeInSeconds" type="Integer"/>
	<property name="status" type="String"/>
	<property name="km" type="Integer"/>
	<property name="cyclist" type="my.cyclingraceFacade.data.CyclistData" />
	</bean>
	
	
	<bean class="my.cyclingraceFacade.data.StageData">
	<property name="code" type="String"/>
	<property name="date" type="java.util.Date"/>
	<property name="start" type="String"/>
	<property name="end" type="String"/>
	<property name="km" type="Integer"/>
	<property name="stageRankingsList" type="java.util.List&lt;my.cyclingraceFacade.data.StageRankingData>" />
	</bean>
	
	<bean class="my.cyclingraceFacade.data.StageRaceData">
	<property name="code" type="String"/>
	<property name="name" type="String"/>
	<property name="year" type="Integer"/>
	<property name="edition" type="String"/>
	<property name="stagesList" type="java.util.List&lt;my.cyclingraceFacade.data.StageData>" />
	</bean>
	

	
	<bean class="my.cyclingraceFacade.data.CyclistRankingData">
	<property name="code" type="String"/>
	<property name="stageRace" type="my.cyclingraceFacade.data.StageRaceData"/>
	<property name="timeTotInSeconds" type="Integer"/>
	<property name="raceRanking" type="Integer"/>
	<property name="cyclist" type="my.cyclingraceFacade.data.CyclistData"/>
	
	</bean>
	
	
	
	
	

</beans>
